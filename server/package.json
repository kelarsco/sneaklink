{
  "name": "sneaklink-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "dev:watch-all": "node --watch server.js",
    "start": "node server.js",
    "test:db": "node utils/testConnection.js",
    "test:facebook": "node utils/testFacebookToken.js",
    "test:google-auth": "node utils/testGoogleAuth.js",
    "pm2": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop sneaklink-backend",
    "pm2:restart": "pm2 restart sneaklink-backend",
    "pm2:logs": "pm2 logs sneaklink-backend",
    "generate-keys": "node scripts/generateApiKeys.js",
    "generate-totp": "node scripts/generateTotpSecret.js",
    "test-totp": "node scripts/testTotp.js",
    "debug-totp": "node scripts/debugTotp.js",
    "verify-totp": "node scripts/verifyTotpSetup.js",
    "update-themes": "node scripts/updateThemesAndCountries.js",
    "update-themes-only": "node scripts/updateThemesAndCountries.js --theme-only",
    "update-store-themes": "node scripts/updateStoreThemes.js",
    "update-countries-only": "node scripts/updateThemesAndCountries.js --country-only",
    "security-check": "npm audit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "postgres:test": "node scripts/test-postgres-connection.js",
    "db:check": "node scripts/check-database-url.js",
    "migrate:data": "node scripts/migrate-mongodb-to-postgres.js",
    "db:create": "powershell -ExecutionPolicy Bypass -File scripts/create-database.ps1",
    "db:create:cmd": "scripts\\create-database.bat",
    "db:create:simple": "scripts\\create-database-simple.bat"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "google-auth-library": "^9.15.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "puppeteer": "^24.33.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "prisma": "5.22.0",
    "nodemon": "^3.0.2"
  }
}
